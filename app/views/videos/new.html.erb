<%= form_with model: @video do |f|%>
  <div class="form-group row">
    <label for="inputEmail3" class="col-sm-2 col-form-label">Youtube Url:</label>
    <div class="col-sm-10">
      <input name="video[link]" class="form-control" type="input">
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-20">
      <%= f.submit "Share", class: "" %>
    </div>
  </div>
<% end %>